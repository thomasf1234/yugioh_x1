CREATE INDEX "index_cards_on_serial_number" ON "cards" ("serial_number");
CREATE UNIQUE INDEX "index_cards_on_name" ON "cards" ("name");
CREATE VIEW non_monsters AS
SELECT c.id as card_id,
       c.name as name,
       c.category as category,
       max(case when p.name = 'property' then p.value end) as property,
       c.description as description,
       c.serial_number as serial_number
FROM cards c
INNER JOIN properties p ON c.id = p.card_id
WHERE c.id IN (
    SELECT card_id FROM properties
    WHERE name IN ('property')
    GROUP BY card_id
    HAVING count(card_id) = 1
  )
AND c.category IN (
  'Spell',
  'Trap'
)
GROUP BY c.id;
CREATE VIEW monsters AS
SELECT c.id as card_id,
       c.name as name,
       c.category as category,
       max(case when p.name = 'element' then p.value end) as element,
       max(case when p.name = 'level' then p.value end) as level,
       max(case when p.name = 'rank' then p.value end) as rank,
       max(case when p.name = 'species' then p.value end) as species,
       c.description as description,
       max(case when p.name = 'attack' then p.value end) as attack,
       max(case when p.name = 'defense' then p.value end) as defense,
       c.serial_number as serial_number
FROM cards c
INNER JOIN properties p ON c.id = p.card_id
WHERE c.id IN (
    SELECT card_id FROM properties
    WHERE name IN (
'element',
'level',
'rank',
'species',
'attack',
'defense'
)
    GROUP BY card_id
    HAVING count(card_id) = 5
  )
AND c.category IN (
  'Normal',
  'Effect',
  'Ritual',
  'Fusion',
  'Synchro',
  'Xyz'
)
GROUP BY c.id;
COMMIT;
